<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.W1956078_CW2"
        tools:targetApi="31" >
        <activity
            android:name=".Jersey"
            android:exported="false"
            android:label="@string/title_activity_jersey"
            android:theme="@style/Theme.W1956078_CW2" />
        <activity
            android:name=".SearchByLeagues"
            android:exported="false"
            android:label="@string/title_activity_search_by_leagues"
            android:theme="@style/Theme.W1956078_CW2" />
        <activity
            android:name=".AddLeague"
            android:exported="false"
            android:label="@string/title_activity_add_league"
            android:theme="@style/Theme.W1956078_CW2" />
        <activity
            android:name=".SearchClub"
            android:exported="false"
            android:label="@string/title_activity_search_club"
            android:theme="@style/Theme.W1956078_CW2" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.W1956078_CW2" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>